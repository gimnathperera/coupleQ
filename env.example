NEXT_PUBLIC_CONVEX_URL=your_convex_url_here

# Image system configuration
# Set to 'internet' to fetch images from Unsplash API, 'local' to use local SVG files
NEXT_PUBLIC_IMAGE_SOURCE=internet

# Optional: Unsplash API key for fetching images from the internet
# Get your free API key from https://unsplash.com/developers
NEXT_PUBLIC_UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here
